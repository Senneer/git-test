doctype
head
    title= page
    meta(charset='utf-8')
    link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css')
body
    .container
        h2= page
        hr
        ul.nav.justify-content-center
            each link in links
                li.nav-link: a(href=link)= link
            li.nav-link: a(href='new') new
        nav
        hr
        block content
